CREATE TABLE SALGRADE (
    GRADE NUMBER(2) UNIQUE NOT NULL,
    LOSAL NUMBER(7,2) NOT NULL,
    HISAL NUMBER(7,2) NOT NULL
    );
    
INSERT INTO SALGRADE VALUES(5,5000,6000);
