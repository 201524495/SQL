/**

*/
SET SERVEROUTPUT ON
DECLARE
    V_START NUMBER := &P_START;
    V_END NUMBER := &P_END;

BEGIN
    IF V_START > V_END THEN
        DBMS_OUTPUT.PUT_LINE('CHANGE START AND END VALUES');
        V_START := V_START + V_END;
        V_END := V_START - V_END;
        V_START := V_START - V_END;
    ELSE 
        DBMS_OUTPUT.PUT_LINE('NOT CHANGE START AND END VALUES');
    END IF;        
        DBMS_OUTPUT.PUT_LINE('START NUM : ' || V_START);
        DBMS_OUTPUT.PUT_LINE('END NUM : ' || V_END);
    LOOP
        DBMS_OUTPUT.PUT_LINE(V_START);
        V_START := V_START + 1;
        EXIT WHEN V_START > V_END;
    END LOOP;
END;
/
SET SERVEROUTPUT OFF