INSERT INTO EMP VALUES(99, 'TEST', 'TEST', 6, SYSDATE, 1600, 500, 40);

SELECT * FROM EMP;

SET VERIFY OFF
ACCEPT P_EMPNO PROMPT ' DELETE EMPNO : ';

DECLARE 
    V_EMPNO NUMBER(4) := &P_EMPNO;

BEGIN
    DELETE EMP
    WHERE EMPNO = V_EMPNO;
    COMMIT;
END;
/
SET VERIFY ON

SELECT * FROM EMP;